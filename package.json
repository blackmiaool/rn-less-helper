{
    "name": "rn-less-helper",
    "displayName": "rn-less-helper",
    "description": "rn-less's helper",
    "version": "0.0.19",
    "publisher": "blackmiaool",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascriptreact"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "rnLess.rnLessExpand",
                "title": "Expand rnless jsx tag"
            }
        ],
        "keybindings": [
            {
                "command": "rnLess.rnLessExpand",
                "key": "ctrl+shift+r"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "eslint": "^3.6.0",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "babel-traverse": "^6.26.0",
        "babylon": "^6.18.0",
        "postcss": "^6.0.10",
        "postcss-less": "^1.1.0"
    }
}
